
# edge_functions_env_example.env
# Copy to each function's folder as .env (or use `supabase secrets set ...`).
# Required for all functions:
SUPABASE_URL=YOUR_SUPABASE_URL
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Required for sign_asset_url (server-side signing):
SUPABASE_SERVICE_ROLE_KEY=YOUR_SERVICE_ROLE_KEY

# Optional app metadata:
PUBLIC_APP_ORIGIN=http://localhost:5173

# Signing defaults (sign_asset_url):
EDGE_SIGN_URL_TTL=600  # seconds

# AI gate defaults (ai_gate):
AI_MAX_CALLS_PER_MINUTE=5
AI_MIN_INTERVAL_MS=2000

# CORS (if you add checks inside functions):
ALLOWED_ORIGINS=http://localhost:5173,https://vibebeat.io
